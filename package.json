    "isbot": "^4.1.0",
    "lucide-react": "^0.371.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-player": "^2.16.0",
    "remix-auth": "^3.6.0",
    "remix-auth-form": "^1.4.0",
    "tailwind-merge": "^2.2.2",
