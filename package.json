    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "install": "^0.13.0",
    "npm": "^10.5.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prisma": "^5.12.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.1.6",
